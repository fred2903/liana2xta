int cnt;
chan approach, texit;
process train01() {
  clock x;
  state
    train0, train1 { x <= 5 }, train2 { x <= 5 }, train3 { x <= 5 };
  init train0;
  trans
    train2 -> train3 {
    },
    train3 -> train0 {
      sync texit?;
      guard x <= 5;
    },
    train1 -> train2 {
      guard x > 2;
    },
    train0 -> train1 {
      sync approach?;
      assign x = 0;
    };
}
system train01;
